@model CoolTaskManagement.DAL.Company

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_TaskLayout.cshtml";
}
@{
    CoolTaskManagement.DAL.User User = (CoolTaskManagement.DAL.User)Session["UserData"];
   
}

<body class="skin-black">

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <section class="panel">
            <header class="panel-heading">
                Edit Employee
            </header>
            <div class="panel-body">
                <div class="form-horizontal tasi-form">


                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyId, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyId, new { @class = "form-control", @enabled = "false" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyName, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyName, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyDescription, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyDescription, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyAddress, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyAddress, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyCity, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyCity, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyZipCode, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyZipCode, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyTelephoneNo, new { @class = "control-label" })
                        </div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(model => model.CompanyTelephoneNo, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CompanyLat, new { @class = "control-label" })
                        </div>
                        <div class="col-md-3">
                            @Html.TextBoxFor(model => model.CompanyLat, new { @class = "form-control", @enabled = "true" })
                        </div>
                        <div class="col-md-3">
                            @Html.TextBoxFor(model => model.CompanyLong, new { @class = "form-control", @enabled = "true" })
                        </div>
                    </div>


                    <div class="form-group">
                        <div class="col-md-offset-3 col-md-4">
                            <button class="btn btn-primary btn-addon btn-sm " type="submit" value="Edit">
                                <i class="fa fa-pencil"></i>
                                Update
                            </button>
                        </div>

                        <div>

                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-3 col-md-4">
                            @Html.ActionLink("Back To Dashboard", "Index")
                        </div>

                        <div>

                        </div>
                    </div>

                </div>
            </div>
        </section>
    }




</body>

